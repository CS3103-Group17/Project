<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.CrawlerUIController">
   <top>
      <Pane fx:id="searchPane" prefHeight="700.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="searchLabel" layoutX="27.0" layoutY="14.0" text="Find information about your travel location" />
            <Separator layoutX="14.0" layoutY="22.0" prefWidth="10.0" />
            <Separator layoutX="258.0" layoutY="23.0" prefWidth="725.0" />
            <TextField fx:id="nameTextField" layoutX="20.0" layoutY="41.0" onKeyPressed="#searchPaneOnKeyPressed" prefHeight="25.0" prefWidth="376.0" promptText="Destination Country / City name" />
            <DatePicker fx:id="arrivalDatePicker" editable="false" layoutX="19.0" layoutY="86.0" onKeyPressed="#searchPaneOnKeyPressed" promptText="Leaving on" />
            <DatePicker fx:id="departureDatePicker" editable="false" layoutX="222.0" layoutY="86.0" onKeyPressed="#searchPaneOnKeyPressed" promptText="Returning on" />
            <TextField fx:id="pointOfInterestTextField" layoutX="19.0" layoutY="132.0" onKeyPressed="#searchPaneOnKeyPressed" prefHeight="25.0" prefWidth="377.0" promptText="Origin Country / City point of interest" />
            <Button fx:id="searchButton" layoutX="453.0" layoutY="132.0" mnemonicParsing="false" onMouseClicked="#searchButtonOnClicked" prefHeight="25.0" prefWidth="68.0" text="Search" />
            <Separator layoutX="14.0" layoutY="170.0" prefHeight="0.0" prefWidth="970.0" />
            <StackPane fx:id="travelLocationStackPane" layoutX="19.0" layoutY="181.0" prefHeight="512.0" prefWidth="241.0">
               <children>
                  <TreeView fx:id="travelLocationTreeView" prefHeight="200.0" prefWidth="200.0" />
               </children></StackPane>
            <StackPane fx:id="displayInformationStackPane" layoutX="260.0" layoutY="181.0" prefHeight="512.0" prefWidth="740.0" />
         </children>
      </Pane>
   </top>
</BorderPane>
